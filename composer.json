{
    "name": "alexanderpavlov/laravel-fullcalendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Pavlov",
            "email": "spb.alexandre@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*"
    },
    "autoload": {
        "psr-0": {
            "MaddHatter\\LaravelFullcalendar": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaddHatter\\LaravelFullcalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "MaddHatter\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
